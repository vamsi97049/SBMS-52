package i.vk52.beans.classes;

public class Employee {
	private String empid;
	private String name;
	private String role;
	private int experience;
	private int salary;
	
	public Employee() {
		System.out.println("Employe::Instantiation Successful / Object has been created for Employee class");
	}
	
	public Employee(String empid, String name, String role, int experience, int salary) {
		super();
		this.empid = empid;
		this.name = name;
		this.role = role;
		this.experience = experience;
		this.salary = salary;
	}
	
	
}
